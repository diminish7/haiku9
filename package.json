{
  "name": "haiku9",
  "version": "1.1.0-beta-01",
  "description": "Asset compilation, static-site generator",
  "main": "lib/index.js",
  "bin": {
    "h9": "bin/h9",
    "h9-server": "bin/h9-serve",
    "h9-compile": "bin/h9-build"
  },
  "dependencies": {
    "aws-sdk": "^2.2.26",
    "browserify": "^12.0.1",
    "chokidar": "^1.2.0",
    "coffeeify": "^2.0.1",
    "commander": "^2.9.0",
    "express": "^4.13.3",
    "fairmont": "^1.0.0-beta-40",
    "js-yaml": "^3.4.0",
    "marked": "^0.3.5",
    "mime": "^1.3.4",
    "mime-types": "^2.1.6",
    "panda-9000": "1.0.0-alpha-06",
    "rimraf": "^2.4.3",
    "when": "^3.7.4"
  },
  "devDependencies": {
    "amen": "^1.0.0-alpha-06",
    "json": "^9.0.3"
  },
  "scripts": {
    "prepublish": "coffee -o lib/ -c src/*.coffee",
    "test": "coffee test/index.coffee",
    "watch": "coffee -o lib/ -cw src/",
    "version": "(node_modules/.bin/json -f package.json version > VERSION)",
    "tag": "(node_modules/.bin/json -f package.json version | tee VERSION | xargs -I version git tag -am version version) && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/pandastrike/haiku9.git"
  },
  "keywords": [
    "blog",
    "jekyll",
    "harp",
    "asset",
    "compilation"
  ],
  "author": "Dan Yoder <dan@pandastrike.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pandastrike/haiku9/issues"
  },
  "homepage": "https://github.com/pandastrike/haiku9"
}
